{
  "name": "welloladev/laravel-cognito-pa-provider",
  "description": "AWS Cognito provider for Laravel 6 LTS Authentication",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "cognito", "aws", "authentication"],
  "authors": [
    {
      "name": "Marco Deleu",
      "email": "deleugyn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-gmp": "*",
    "ext-json": "*",
    "web-token/jwt-checker": "^2.0",
    "web-token/jwt-core": "^2.0",
    "web-token/jwt-signature": "^2.0",
    "web-token/jwt-signature-algorithm-rsa": "^2.0",
    "illuminate/contracts": "^6.0 || ^7.1 || ^8.0"
  },
  "require-dev": {
    "illuminate/cache": "^6.0 || ~7.1 || ^8.0",
    "illuminate/config": "^6.0 || ~7.1 || ^8.0",
    "illuminate/container": "^6.0 || ~7.1 || ^8.0",
    "phpunit/phpunit": "^9.0",
    "doctrine/coding-standard": "^6.0 || ^8.0",
    "phpstan/phpstan": "^0.12.4",
    "web-token/jwt-easy": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Wellola\\PACognito\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\CustomerGauge\\Cognito\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wellola\\PACognito\\LaravelCognitoServiceProvider"
      ]
    }
  }
}
